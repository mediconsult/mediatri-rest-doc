    <wadl:resource path="api/v1/diagnoses/codes">
      <wadl:method name="GET">
        <wadl:doc><![CDATA[Return the available code based on the search pattern.
Use search API carefully when using paging.
All parameters for page > 1 should be the same as in the request for the first page.]]></wadl:doc>
        <wadl:request>
          <wadl:param name="age" style="query">
            <wadl:doc><![CDATA[age of the patient which you want to use codes for. It can be empty and no restrictions by age.]]></wadl:doc>
          </wadl:param>
          <wadl:param name="baseCode" style="query">
            <wadl:doc><![CDATA[base code for another type than DiagnoseCodeType#Basic. It is obligatory parameter for such types.
Server will generate the validation error if it is null. Base code can contain the syyskoodi and oirekoodi separated by space.]]></wadl:doc>
          </wadl:param>
          <wadl:param name="page" style="query">
            <wadl:doc><![CDATA[number of page in the case when more than DiagnoseCodeList#DEFAULT_CODE_COUNT codes are available for some pattern and type.
Client can ask another page for the codes for the same pattern and type. Can be null and always the first page will be loaded.]]></wadl:doc>
          </wadl:param>
          <wadl:param name="pattern" style="query">
            <wadl:doc><![CDATA[search pattern entered by users. String should be separated by spaces.]]></wadl:doc>
          </wadl:param>
          <wadl:param name="sex" style="query">
            <wadl:doc><![CDATA[sex for the code search. It can be absent and no filter by sex in this case.]]></wadl:doc>
          </wadl:param>
          <wadl:param name="type" style="query">
            <wadl:doc><![CDATA[of code to search. Please see DiagnoseCodeType for more detail]]></wadl:doc>
          </wadl:param>
        </wadl:request>
        <wadl:response>
          <wadl:doc><![CDATA[instance of DiagnoseCodeList object]]></wadl:doc>
          <wadl:representation mediaType="application/json"/>
        </wadl:response>
      </wadl:method>
    </wadl:resource>

