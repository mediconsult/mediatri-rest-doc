    <wadl:resource path="api/v1/hoitotyo/{id}/endShiftDataEdit/{shiftId}/{started}">
      <wadl:method name="GET">
        <wadl:doc><![CDATA[Unlocks a given shift data row after editing.]]></wadl:doc>
        <wadl:request>
          <wadl:param name="isDataChanged" style="query">
            <wadl:doc><![CDATA[<code>true</code> if data was actually changed during locking, otherwise <code>false</code>.
