    <wadl:resource path="api/v1/users/search">
      <wadl:method name="GET">
        <wadl:doc><![CDATA[Makes search from all users with given criteria.
Either none or all search parameters can be set.]]></wadl:doc>
        <wadl:request>
          <wadl:param name="endTime" style="query">
            <wadl:doc><![CDATA[availability condition (user account validity end time, inclusive), or <code>null</code> if not used as criterion]]></wadl:doc>
          </wadl:param>
          <wadl:param name="firstNamePrefix" style="query">
            <wadl:doc><![CDATA[first name starts with condition, or <code>null</code> if not used as criterion]]></wadl:doc>
          </wadl:param>
          <wadl:param name="fullNameContains" style="query">
            <wadl:doc><![CDATA[user's full name contains string condition, or <code>null</code> if not used as criterion
Full name search overrides more specific lastNamePrefix, firstNamePrefix, userNamePrefix searches.]]></wadl:doc>
          </wadl:param>
          <wadl:param name="lastNamePrefix" style="query">
            <wadl:doc><![CDATA[last name starts with condition, or <code>null</code> if not used as criterion]]></wadl:doc>
          </wadl:param>
          <wadl:param name="list" style="query">
            <wadl:doc><![CDATA[optional user list id(s), if not present users are retrieved from global register]]></wadl:doc>
          </wadl:param>
          <wadl:param name="pageIndex" style="query">
            <wadl:doc><![CDATA[page index starting from which client wants to list users]]></wadl:doc>
          </wadl:param>
          <wadl:param name="pageSize" style="query">
            <wadl:doc><![CDATA[defines page size i.e. maximum amount of users returned in on query]]></wadl:doc>
          </wadl:param>
          <wadl:param name="skill" style="query">
            <wadl:doc><![CDATA[skill(s) supported condition, or an empty list if not used as criterion]]></wadl:doc>
          </wadl:param>
          <wadl:param name="startTime" style="query">
            <wadl:doc><![CDATA[availability condition (user account validity start time, inclusive), or <code>null</code> if not used as criterion]]></wadl:doc>
          </wadl:param>
          <wadl:param name="unit" style="query">
            <wadl:doc><![CDATA[optional unit ids, if present users lists for given units are retrieved, otherwise login unit is assumed]]></wadl:doc>
          </wadl:param>
          <wadl:param name="userNamePrefix" style="query">
            <wadl:doc><![CDATA[username starts with condition, or <code>null</code> if not used as criterion]]></wadl:doc>
          </wadl:param>
        </wadl:request>
        <wadl:response status="400">
          <wadl:doc><![CDATA[Bad Request: if values were invalid]]></wadl:doc>
        </wadl:response>
        <wadl:response status="401">
          <wadl:doc><![CDATA[Unauthenticated: The request requires user authentication. ]]></wadl:doc>
        </wadl:response>
        <wadl:response status="403">
          <wadl:doc><![CDATA[Forbidden: User is authenticated but doesn't have enough priviledges to access this data. Re-login will not help and the request SHOULD NOT be repeated.]]></wadl:doc>
        </wadl:response>
        <wadl:response status="500">
          <wadl:doc><![CDATA[Unexpected error]]></wadl:doc>
        </wadl:response>
        <wadl:response>
          <wadl:doc><![CDATA[users found with given criteria]]></wadl:doc>
          <wadl:representation mediaType="application/json"/>
        </wadl:response>
      </wadl:method>
    </wadl:resource>

