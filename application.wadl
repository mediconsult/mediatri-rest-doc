        <wadl:request>
          <wadl:param name="endTime" style="query">
            <wadl:doc><![CDATA[list members should be valid before this time (inclusive)]]></wadl:doc>
          </wadl:param>
          <wadl:param name="includePassive" style="query">
            <wadl:doc><![CDATA[should passive patients be included in the list? (false by default) NB! Patient's status (passive
or not) is checked in reference to start time, if given, or current time, if not.]]></wadl:doc>
          </wadl:param>
          <wadl:param name="list" style="query">
            <wadl:doc><![CDATA[patient list form id]]></wadl:doc>
          </wadl:param>
          <wadl:param name="startTime" style="query">
            <wadl:doc><![CDATA[list members should be valid after this time (inclusive)]]></wadl:doc>
          </wadl:param>
          <wadl:param name="unit" style="query">
            <wadl:doc><![CDATA[unit ids for which list contents should be fetched]]></wadl:doc>
          </wadl:param>
        </wadl:request>
        <wadl:response>
          <wadl:doc><![CDATA[a list of patient lists grouped by (unit id, list id)]]></wadl:doc>
          <wadl:representation mediaType="application/json"/>
        </wadl:response>
      </wadl:method>
    </wadl:resource>

    <wadl:resource path="api/v1/patients/search">
      <wadl:method name="GET">
        <wadl:doc><![CDATA[Makes search from all patients with given criteria.
Either none or all search parameters can be set.]]></wadl:doc>
        <wadl:request>
          <wadl:param name="firstNamePrefix" style="query">
            <wadl:doc><![CDATA[first name starts with condition, or <code>null</code> if not used as criterion]]></wadl:doc>
          </wadl:param>
          <wadl:param name="fullNameContains" style="query">
            <wadl:doc><![CDATA[patient's full name contains string condition, or <code>null</code> if not used as criterion
Full name search overrides more specific lastNamePrefix, firstNamePrefix, userNamePrefix searches.
A little bit unexpectedly full name contain search also include also match in case search string
is part of patient's SSN i.e. henkilötunnus.]]></wadl:doc>
          </wadl:param>
          <wadl:param name="lastNamePrefix" style="query">
            <wadl:doc><![CDATA[last name starts with condition, or <code>null</code> if not used as criterion]]></wadl:doc>
          </wadl:param>
          <wadl:param name="list" style="query">
            <wadl:doc><![CDATA[optional patient list id(s), if not present patients are retrieved from global register]]></wadl:doc>
          </wadl:param>
          <wadl:param name="pageIndex" style="query">
            <wadl:doc><![CDATA[page index starting from which client wants to list patients]]></wadl:doc>
          </wadl:param>
          <wadl:param name="pageSize" style="query">
            <wadl:doc><![CDATA[defines page size i.e. maximum amount of patients returned in on query, if page size is set, then also
<code>pageIndex</code> parameter must be defined. If not set, returning all the patients
matching given conditions.]]></wadl:doc>
          </wadl:param>
          <wadl:param name="ssnPrefix" style="query">
            <wadl:doc><![CDATA[social security starts with condition, or <code>null</code> if not used as criterion]]></wadl:doc>
          </wadl:param>
          <wadl:param name="unit" style="query">
            <wadl:doc><![CDATA[optional unit ids, if present patient lists for given units are retrieved, otherwise login unit is assumed]]></wadl:doc>
          </wadl:param>
        </wadl:request>
        <wadl:response>
          <wadl:doc><![CDATA[patients found with given criteria]]></wadl:doc>
          <wadl:representation mediaType="application/json"/>
        </wadl:response>
      </wadl:method>
    </wadl:resource>

    <wadl:resource path="api/v1/patients/{id}">
      <wadl:method name="GET">
        <wadl:doc><![CDATA[This fetches patient by id.]]></wadl:doc>
        <wadl:request>
          <wadl:param name="includeBasicInfo" style="query">
            <wadl:doc><![CDATA[if true, data is pre-loaded, otherwise accessible via lazy load URL]]></wadl:doc>
          </wadl:param>
          <wadl:param name="includeCareRelationPreferenceList" style="query">
            <wadl:doc><![CDATA[if true, data is pre-loaded, otherwise accessible via lazy load URL]]></wadl:doc>
          </wadl:param>
          <wadl:param name="includeHoitojakso" style="query">
            <wadl:doc><![CDATA[]]></wadl:doc>
