    <wadl:resource path="api/v1/hoitotyo/{id}/endShift/{shiftId}">
      <wadl:method name="GET">
        <wadl:doc><![CDATA[Ends shift by marking shift data as locked.]]></wadl:doc>
        <wadl:request>
          <wadl:param name="lastReadTime" style="query">
            <wadl:doc><![CDATA[time of latest read by client (used to detect if there are newer changes).
Optional parameter, so no edit in-between checks made, if <code>null</code>.
Timestamp is of format "yyyy-MM-ddTHH:mm:ss.SSS", optionally complemented with time zone information.]]></wadl:doc>
          </wadl:param>
        </wadl:request>
        <wadl:response status="400">
          <wadl:doc><![CDATA[Bad Request: if values were invalid e.g. no shift with given id was found.]]></wadl:doc>
        </wadl:response>
        <wadl:response status="401">
          <wadl:doc><![CDATA[Unauthenticated: The request requires user authentication. ]]></wadl:doc>
        </wadl:response>
        <wadl:response status="403">
          <wadl:doc><![CDATA[Forbidden: User is authenticated but doesn't have enough priviledges to access this data. Re-login will not help and the request SHOULD NOT be repeated.]]></wadl:doc>
        </wadl:response>
        <wadl:response status="409">
          <wadl:doc><![CDATA[Conflict: Shift has been already ended, or some other user has edited data, so there is newer data available.]]></wadl:doc>
        </wadl:response>
        <wadl:response status="423">
          <wadl:doc><![CDATA[Failure to end shift and lock the shift data, because some other user is editing the data.]]></wadl:doc>
        </wadl:response>
        <wadl:response status="500">
          <wadl:doc><![CDATA[Unexpected error]]></wadl:doc>
        </wadl:response>
        <wadl:response>
          <wadl:doc><![CDATA[hoitotyo dto for the ended hoitotyo shift instance]]></wadl:doc>
          <wadl:representation mediaType="application/json"/>
        </wadl:response>
      </wadl:method>
      <wadl:param name="shiftId" style="template">
        <wadl:doc><![CDATA[shift id]]></wadl:doc>
      </wadl:param>
      <wadl:param name="id" style="template">
        <wadl:doc><![CDATA[patient id]]></wadl:doc>
      </wadl:param>
    </wadl:resource>

    <wadl:resource path="api/v1/hoitotyo/{id}/startShiftDataEdit/{shiftId}">
      <wadl:method name="GET">
        <wadl:doc><![CDATA[Locks a given shift data row for editing.]]></wadl:doc>
        <wadl:request>
          <wadl:param name="lastReadTime" style="query">
            <wadl:doc><![CDATA[time of latest read by client (used to detect if there are newer changes).
Optional parameter, so no edit in-between checks made, if <code>null</code>.
Timestamp is of format "yyyy-MM-ddTHH:mm:ss.SSS", optionally complemented with time zone information.]]></wadl:doc>
          </wadl:param>
        </wadl:request>
        <wadl:response status="400">
          <wadl:doc><![CDATA[Bad Request: if values were invalid e.g. no shift with given id was found.]]></wadl:doc>
        </wadl:response>
        <wadl:response status="401">
          <wadl:doc><![CDATA[Unauthenticated: The request requires user authentication. ]]></wadl:doc>
        </wadl:response>
        <wadl:response status="403">
          <wadl:doc><![CDATA[Forbidden: User is authenticated but doesn't have enough priviledges to access this data. Re-login will not help and the request SHOULD NOT be repeated.]]></wadl:doc>
        </wadl:response>
        <wadl:response status="409">
          <wadl:doc><![CDATA[Conflict: Some other user has edited data, so there is newer data available.]]></wadl:doc>
        </wadl:response>
        <wadl:response status="423">
          <wadl:doc><![CDATA[Data is locked already by some other user.]]></wadl:doc>
        </wadl:response>
        <wadl:response status="500">
          <wadl:doc><![CDATA[Unexpected error]]></wadl:doc>
        </wadl:response>
        <wadl:response>
          <wadl:doc><![CDATA[timestamp in string format ("yyyy-MM-ddTHH:mm:ss.SSS", optionally complemented with time zone information)
identifying lock object (together with omistaja and shift id).]]></wadl:doc>
          <wadl:representation mediaType="application/json"/>
        </wadl:response>
      </wadl:method>
      <wadl:param name="shiftId" style="template">
        <wadl:doc><![CDATA[shift id to be locked.]]></wadl:doc>
      </wadl:param>
      <wadl:param name="id" style="template">
        <wadl:doc><![CDATA[patient id]]></wadl:doc>
      </wadl:param>
    </wadl:resource>

